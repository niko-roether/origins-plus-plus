    }
  },
  "iron_golem_no": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:ravager/iron_golem_no {power:\"power\",resource:\"resource\"}"
    }
  }
}
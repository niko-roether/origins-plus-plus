{
  "type": "origins:multiple",
  "hidden": true,
  "empty": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:damage",
      "amount": 60,
      "damage_type": "wither"
    },
    "interval": 1,
    "condition": {
      "type": "origins:resource",
      "resource": "origins-plus-plus:end-mage/resource",
      "comparison": "<",
      "compare_to": 1
    }
  },
  "refill": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:end-mage/resource",
      "change": 1
    },
    "interval": 7,
    "condition": {
      "type": "origins:resource",
      "resource": "origins-plus-plus:end-mage/resource",
      "comparison": "<",
      "compare_to": 1000
    }
  },
  "regen": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:end-mage/resource",
      "change": 1
    },
    "interval": 4,
    "condition": {
      "type": "origins:resource",
      "resource": "origins-plus-plus:end-mage/resource",
      "comparison": "<",
      "compare_to": 800
    }
  },
  "respawn": {
    "type": "origins:action_on_callback",
    "entity_action_respawned": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:end-mage/resource",
      "change": 200,
      "operation": "set"
    },
    "low": {
      "type": "origins:action_over_time",
      "entity_action": {
        "type": "origins:execute_command",
        "command": "function origins-plus-plus:end-mage/bug"
      },
      "interval": 200,
      "condition": {
        "type": "origins:resource",
        "resource": "origins-plus-plus:end-mage/resource",
        "comparison": "<",
        "compare_to": 100
      }
    }
  }
}
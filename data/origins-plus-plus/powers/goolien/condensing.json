{
  "type": "origins:multiple",
  "resource": {
    "type": "origins:resource",
    "min": 0,
    "max": 1,
    "hud_render": {
      "should_render": false
    }
  },
  "switch": {
    "type": "origins:active_self",
    "key": "key.origins.primary_active",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_resource",
        "comparison": "==",
        "compare_to": 0
      },
      "if_action": {
        "type": "origins:execute_command",
        "command": "function origins-plus-plus:goolien/condense {power:\"power\",resource:\"resource\"}"
      },
      "else_action": {
        "type": "origins:execute_command",
        "command": "function origins-plus-plus:goolien/reset {power:\"power\",resource:\"resource\"}"
      }
    }
  },
  "power_lost": {
    "type": "origins:action_on_callback",
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:goolien/reset {power:\"power\",resource:\"resource\"}"
    }
  }
}
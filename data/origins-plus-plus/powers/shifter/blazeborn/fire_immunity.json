{
  "type": "origins:multiple",
  "firebar": {
    "type": "origins:resource",
    "min": 0,
    "max": 11,
    "hud_render": {
      "bar_index": 7,
      "condition": {
        "type": "origins:resource",
        "resource": "origins-plus-plus:shifter/shift_bar",
        "comparison": "==",
        "compare_to": 8
      }
    }
  },
  "firebar2": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:shifter/blazeborn/fire_immune_firebar",
      "change": 1
    },
    "interval": 20,
    "condition": {
      "type": "origins:all_of",
      "conditions": [
        {
          "type": "origins:on_fire"
        },
        {
          "type": "origins:resource",
          "resource": "origins-plus-plus:shifter/shift_bar",
          "comparison": "==",
          "compare_to": 8
        }
      ]
    }
  },
  "firebar3": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:shifter/fireimmune"
    },
    "interval": 20,
    "condition": {
      "type": "origins:all_of",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "origins-plus-plus:shifter/blazeborn/fire_immune_firebar",
          "comparison": "<=",
          "compare_to": 9
        },
        {
          "type": "origins:resource",
          "resource": "origins-plus-plus:shifter/shift_bar",
          "comparison": "==",
          "compare_to": 8
        }
      ]
    }
  },
  "firebar4": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:shifter/blazeborn/fire_immune_firebar",
      "change": -1
    },
    "interval": 40,
    "condition": {
      "type": "origins:all_of",
      "conditions": [
        {
          "type": "origins:on_fire",
          "inverted": true
        },
        {
          "type": "origins:resource",
          "resource": "origins-plus-plus:shifter/shift_bar",
          "comparison": "==",
          "compare_to": 8
        }
      ]
    }
  }
}
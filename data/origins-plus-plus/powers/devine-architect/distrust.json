{
  "type": "origins:multiple",
  "illager": {
    "type": "origins:modify_type_tag",
    "tag": "illager"
  },
  "high_prices": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:devine-architect/high_prices {power:\"power\",resource:\"resource\"}"
    }
  },
  "iron_golem": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:devine-architect/iron_golem {power:\"power\",resource:\"resource\"}"
    }
  },
  "iron_golem_no": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:devine-architect/iron_golem_no {power:\"power\",resource:\"resource\"}"
    }
  },
  "no_pets": {
    "type": "origins:prevent_entity_use",
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:in_tag",
        "tag": "origins-plus-plus:pets"
      }
    }
  }
}
{
  "type": "origins:multiple",
  "join_team": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:zero-aizawa/nice_mobs_join {power:\"power\",resource:\"resource\"}"
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "team leave @s"
    }
  },
  "add_mobs": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:zero-aizawa/nice_mobs_add_mobs {power:\"power\",resource:\"resource\"}"
    },
    "interval": 20
  },
  "hostile_when_hit": {
    "type": "origins:target_action_on_hit",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "tag @s[team=zero_aizawa,type=!minecraft:player] add angry"
    },
    "cooldown": 1
  }
}
{
  "type": "origins:action_over_time",
  "interval": 1,
  "entity_action": {
    "type": "origins:if_else",
    "condition": {
      "type": "origins:sneaking"
    },
    "if_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:binturong/crouch {power:\"power\",resource:\"resource\"}"
    },
    "else_action": {
      "type": "origins:execute_command",
      "command": "function origins-plus-plus:binturong/size {power:\"power\",resource:\"resource\"}"
    }
  }
}
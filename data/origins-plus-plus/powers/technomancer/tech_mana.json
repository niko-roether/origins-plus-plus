{
  "type": "origins:multiple",
  "resource": {
    "type": "origins:resource",
    "min": 0,
    "max": 20000,
    "hud_render": {
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
      "bar_index": 14
    }
  },
  "passive_gain": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:technomancer/tech_mana_resource",
      "change": 20
    },
    "interval": 20,
    "condition": {
      "type": "origins:all_of",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:in_rain",
          "inverted": true
        }
      ]
    }
  },
  "high_passive_gain": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:technomancer/tech_mana_resource",
      "change": 300
    },
    "interval": 10,
    "condition": {
      "type": "origins:all_of",
      "conditions": [
        {
          "type": "origins:on_block",
          "block_condition": {
            "type": "origins:in_tag",
            "tag": "origins-plus-plus:redstone_blocks"
          }
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:in_rain",
          "inverted": true
        }
      ]
    }
  },
  "charged_particle_effects": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "execute at @s run particle minecraft:dust 255 0 0 1 ~ ~0.9 ~ .25 .25 .26 0.01 3 normal @a"
    },
    "interval": 10,
    "condition": {
      "type": "origins:all_of",
      "conditions": [
        {
          "type": "origins:on_block",
          "block_condition": {
            "type": "origins:in_tag",
            "tag": "origins-plus-plus:redstone_blocks"
          }
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:in_rain",
          "inverted": true
        }
      ]
    }
  },
  "charged_particle": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle minecraft:dust 255 0 0 1 ~ ~0.5 ~ .25 .25 .26 0 2 force @a"
    },
    "interval": 1,
    "condition": {
      "type": "origins:resource",
      "resource": "origins-plus-plus:technomancer/tech_mana_resource",
      "comparison": ">",
      "compare_to": 7000
    }
  },
  "water_mana_loss": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "origins-plus-plus:technomancer/tech_mana_resource",
      "change": -300
    },
    "interval": 10,
    "condition": {
      "type": "origins:any_of",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:in_rain"
        }
      ]
    }
  },
  "death": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "resource set @s origins-plus-plus:technomancer/tech_mana_resource 0"
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "resource set @s origins-plus-plus:technomancer/tech_mana_resource 0"
    }
  }
}
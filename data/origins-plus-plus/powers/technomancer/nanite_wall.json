{
  "type": "origins:multiple",
  "summon_nanites": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "execute as @s run function origins-plus-plus:technomancer/wall_summon {power:\"power\",resource:\"resource\"}"
    },
    "condition": {
      "type": "origins:power_active",
      "power": "origins-plus-plus:technomancer/nanite_wall_toggle"
    },
    "key": "key.use"
  },
  "immobile": {

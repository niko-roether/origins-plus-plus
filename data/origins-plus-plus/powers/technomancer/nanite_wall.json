    "duration_per_stack": 20,
    "effects": [
      {
        "amplifier": 255,
        "show_particles": false,
        "show_icon": false,
        "id": "minecraft:jump_boost"
      },
      {
        "amplifier": 255,
        "show_particles": false,
        "show_icon": false,
        "id": "minecraft:slowness"
      },
      {
        "amplifier": 255,
